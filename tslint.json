{
    "extends": "tslint:recommended",
    "rulesDirectory": [ "codelyzer" ],
    "rules": {
        "adjacent-overload-signatures": true,
        "align": true,
        "arrow-parens": true,
        "await-promise": true,
        "array-type": false,
        "arrow-return-shorthand": true,
        "ban": false,
        "ban-comma-operator": true,
        "ban-types": false,
        "binary-expression-operand-order": true,
        "callable-types": false,
        "class-name": true,
        "comment-format": true,
        "completed-docs": true,
        "component-class-suffix": true,
        "component-max-inline-declarations": true,
        "component-selector": [ true , "element" , "wolf" , "kebab-case" ],
        "contextual-lifecycle": true,
        "curly": true,
        "cyclomatic-complexity": true,
        "deprecation": true,
        "directive-class-suffix": true,
        "directive-selector": [ true , "attribute" , "wolf" , "camelCase" ],
        "encoding": true,
        "eofline": true,
        "file-header": false,
        "forin": false,
        "import-blacklist": false,
        "import-spacing": true,
        "indent": [ true , "spaces" , 4 ],
        "interface-name": true,
        "interface-over-type-literal": true,
        "jsdoc-format": true,
        "label-position": true,
        "linebreak-style": false,
        "max-classes-per-file": [ true , 1 ],
        "member-access": true,
        "member-ordering": true,
        "max-line-length": { "options": [ 1250 ] },
        "match-default-export-name": true,
        "max-file-line-count": false,
        "new-parens": true,
        "newline-before-return": true,
        "newline-per-chained-call": false,
        "no-attribute-decorator": true,
        "no-conflicting-lifecycle": true,
        "no-forward-ref": true,
        "no-host-metadata-property": true,
        "no-input-rename": true,
        "no-inputs-metadata-property": true,
        "no-lifecycle-call": true,
        "no-output-native": true,
        "no-output-on-prefix": true,
        "no-output-rename": true,
        "no-outputs-metadata-property": true,
        "no-pipe-impure": true,
        "no-queries-metadata-property": true,
        "no-unused-css": true,
        "no-angle-bracket-type-assertion": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-boolean-literal-compare": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": { "severity": "warning" , "options": [ "debug" , "info" , "log" , "time" , "timeEnd" , "trace" ] },
        "no-debugger": true,
        "no-duplicate-imports": true,
        "no-duplicate-super": true,
        "no-duplicate-switch-case": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-floating-promises": false,
        "no-implicit-dependencies": false,
        "no-import-side-effect": false,
        "no-internal-module": true,
        "no-invalid-this": true,
        "no-irregular-whitespace": true,
        "no-magic-numbers": false,
        "no-mergeable-namespace": true,
        "no-misused-new": true,
        "no-namespace": true,
        "no-parameter-reassignment": true,
        "no-redundant-jsdoc": false,
        "no-reference": true,
        "no-require-imports": true,
        "no-return-await": true,
        "no-shadowed-variable": true,
        "no-sparse-arrays": true,
        "no-string-literal": true,
        "no-string-throw": true,
        "no-submodule-imports": false,
        "no-switch-case-fall-through": false,
        "no-this-assignment": true,
        "no-trailing-whitespace": true,
        "no-unnecessary-type-assertion": true,
        "no-unsafe-finally": true,
        "no-unused-expression": true,
        "no-unused-variable": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "no-any": false,
        "no-construct": true,
        "no-default-export": false,
        "no-dynamic-delete": true,
        "no-empty": false,
        "no-empty-interface": true,
        "no-for-in-array": false,
        "no-inferrable-types": false,
        "no-inferred-empty-object-type": true,
        "no-invalid-template-strings": true,
        "no-non-null-assertion": false,
        "no-null-keyword": false,
        "no-object-literal-type-assertion": false,
        "no-parameter-properties": false,
        "no-reference-import": true,
        "no-unbound-method": true,
        "no-unnecessary-callback-wrapper": true,
        "no-unnecessary-class": false,
        "no-unnecessary-initializer": false,
        "no-unnecessary-qualifier": true,
        "no-unsafe-any": false,
        "no-var-requires": true,
        "no-void-expression": false,
        "number-literal-format": true,
        "object-literal-key-quotes": false,
        "object-literal-shorthand": true,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "only-arrow-functions": false,
        "one-line": true,
        "one-variable-per-declaration": true,
        "prefer-const": true,
        "prefer-conditional-expression": true,
        "prefer-for-of": true,
        "prefer-function-over-method": false,
        "prefer-inline-decorator": true,
        "prefer-method-signature": true,
        "prefer-object-spread": true,
        "prefer-output-readonly": true,
        "prefer-readonly": true,
        "prefer-switch": true,
        "prefer-template": true,
        "prefer-while": true,
        "promise-function-async": true,
        "quotemark": false,
        "radix": true,
        "restrict-plus-operands": false,
        "return-undefined": true,
        "semicolon": true,
        "space-before-function-paren": false,
        "space-within-parens": false,
        "strict-boolean-expressions": false,
        "strict-type-predicates": false,
        "switch-default": true,
        "switch-final-break": true,
        "template-banana-in-box": true,
        "template-conditional-complexity": [ true , 4 ],
        "template-cyclomatic-complexity": [ true , 5 ],
        "template-i18n": [ true , "check-id" , "check-text" ],
        "template-no-negated-async": true,
        "template-use-track-by-function": true,
        "trailing-comma": false,
        "triple-equals": true,
        "type-literal-delimiter": false,
        "typedef": true,
        "typedef-whitespace": false,
        "typeof-compare": true,
        "unified-signatures": true,
        "use-isnan": true,
        "use-component-selector": true,
        "use-component-view-encapsulation": true,
        "use-default-type-parameter": false,
        "use-lifecycle-interface": true,
        "use-pipe-transform-interface": true,
        "variable-name": true,
        "whitespace": [ true , "array-type", "arrow-parens", "check-branch" , "check-decl" , "check-operator" , "check-module" , "check-separator", "check-rest-spread", "check-type" , "check-typecast" , "check-type-operator" ]
    }
}
